{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"declaration": true,
		"declarationMap": true,
		"outDir": "./dist",
		"moduleResolution": "node",
		"rootDir": "./",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"explainFiles": true,
		"listEmittedFiles": true,
		"traceResolution": true,
		"disableSourceOfProjectReferenceRedirect": true
	},
	"references": [
		{
			"path": "./libs/testLib"
		}
	]
}
